{
  "name": "phrontend-webpack",
  "version": "0.0.7",
  "description": "A webpack config maker for phrontend apps",
  "main": "lib/webpack-config.js",
  "scripts": {
    "build": "babel src --out-dir lib --copy-files --loose",
    "cover": "istanbul cover _mocha -- --compilers js:babel/register -R spec",
    "lint": "eslint src/",
    "prepublish": "node ./utils/prepublish.js",
    "test": "mocha --compilers js:babel/register",
    "watch": "babel src --out-dir lib --copy-files --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flipkart-incubator/phrontend-webpack"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=3.0.0"
  },
  "keywords": [
    "phrontend",
    "phrontend-webpack",
    "webpack"
  ],
  "license": "Apache-2.0",
  "bugs": "https://github.com/flipkart-incubator/phrontend-webpack/issues",
  "homepage": "https://github.com/flipkart-incubator/phrontend-webpack",
  "dependencies": {
    "babel-core": "~5.8.23",
    "babel-loader": "~5.3.2",
    "css-loader": "~0.16.0",
    "cssnext": "~1.8.4",
    "express": "~4.13.3",
    "express-http-proxy": "~0.6.0",
    "extract-text-webpack-plugin": "~0.8.2",
    "image-size-loader": "~0.7.0",
    "imports-loader": "~0.6.4",
    "json-loader": "~0.5.2",
    "node-libs-browser": "~0.5.2",
    "postcss-loader": "^0.5.0",
    "postcss-mixins": "^0.3.0",
    "postcss-nested": "^0.3.2",
    "postcss-simple-vars": "^0.3.0",
    "promise": "~7.0.4",
    "react-hot-loader": "~1.3.0",
    "style-loader": "~0.12.3",
    "webpack": "~1.12.0",
    "webpack-dev-middleware": "~1.2.0",
    "webpack-dev-server": "~1.10.1"
  },
  "devDependencies": {
    "babel": "~5.8.23",
    "babel-eslint": "~4.1.0",
    "eslint": "~1.3.1",
    "eslint-config-phrontend": "0.0.2",
    "istanbul": "~0.3.19",
    "mocha": "~2.3.0",
    "should": "~7.1.0"
  }
}
